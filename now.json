{
  "name": "spectrum-clone",
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "client/package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
